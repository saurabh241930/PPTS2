<html>
  <title>PPST</title>
  <head>
     <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link href="https://fonts.googleapis.com/css?family=Nunito:900" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css?family=Raleway" rel="stylesheet">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
  <link href="https://fonts.googleapis.com/css?family=Nunito:700" rel="stylesheet">
   <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
  <link href="https://fonts.googleapis.com/css?family=Fredoka+One" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css?family=Raleway" rel="stylesheet">
   <link href="https://fonts.googleapis.com/css?family=Cabin" rel="stylesheet">
  </head>
  
  <body>
    
    <div class="row">
      
      <div class="col col-xl-3 col-lg-2 col-md-3 col-sm-3 col-xs-12" style="background-color:#2d2d2d;
                                                                            height:100%;
                                                                            text-align:center;
                                                                            font-family: 'Cabin', sans-serif;
                                                                            position:fixed; 
                                                                            border-right:solid 1px #ccc
                                                                            ">
        
   <div class="col col-xl-12 col-lg-12 col-md-3 col-sm-3 col-xs-12" style=";background-color:#286090;width:107%">
          <h2 style="color: white"><strong>PPTS</strong></h2>
          <p style="font-weight: lighter;font-size: 14px;color: #ccc">पालघर पोलीस  ट्रान्सफर सिस्टिम </p>
        </div>


        <img src="https://en.indietrainers.com/media/images/empty-profile.png" alt="" 
             style="height:100px;width:100px;border-radius:;border:solid 10px #2d2d2d"><br>
        
   <a type="button" class="btn btn-primary" style="opacity:0.9;
                                                   margin-top:0px;
                                                   background-color:rgba(40,96,144,0.7);
                                                  
                                                   ">Add Profile Image</a>
     
        
        <ul class="list-group" style="text-align:center;margin-left:18px;margin-top:20px">
  <li class="list-group-item list-group-item-info" style="width:100%"><strong style="float:left">*Name / नाॉव : </strong><br>
    <span style="font-size:16px;color:black"><%=currentUser.fullName%></span></li>
   <li class="list-group-item list-group-item-info"><strong style="float:left">*Phone No. / फोन : </strong><br>
    <span style="font-size:17px;color:black"><%=currentUser.username%></span></li>
   
   <li class="list-group-item list-group-item-info"><strong style="float:left">*Branch / शाखा : </strong><br>
    <span style="font-size:17px;color:black"><%=currentUser.currentBranch.BranchName%></span></li>
          <li class="list-group-item list-group">
      <a type="button" class="btn btn-success">View complete profile <i class="fa fa-arrow-right"></i></a>
</li>  

</ul>
        
        <div style="">
          <div class="btn-group-vertical" >
  <button type="button" class="btn btn-primary">Apply <i class="fa fa-arrow-right"></i></button>
<button type="button" class="btn btn-primary">Status <i class="fa fa-user-cog"></i></button>
<button type="button" class="btn btn-primary">Notifications <i class="fa fa-bell"></i></button>
<button type="button" class="btn btn-danger">Logout <i class="fa fa-sign-out"></i></button>
</div>
        </div>
        
      </div>
      
        <div class="col col-xl-9 col-lg-10 col-md-9 col-sm-9 col-xs-12" style="background-color:;
                                                                            height:1200px;
                                                                            float:right;
                                                                            font-family: 'Cabin', sans-serif;
                                                                           ">
         
      <div class="col col-xl-2 col-lg-2 col-md-9 col-sm-9 col-xs-12" style="background-color:white;position: fixed;
      left: 270px;
      top: 100px">      
<ul style="list-style-type: none;text-align:center;width:200px;margin-left:-50px">
  
  
  
   <a class="list-group-item"  href="/homepage"><li >All Stations</li></a>
  <a class="list-group-item <%if(location=="Vasai"){%>active<%}%>" href="/branches/Vasai"><li>Vasai</li></a>
  <a class="list-group-item <%if(location=="Nalasophara"){%>active<%}%>" href="/branches/Nalasophara"><li >Nalasophara</li></a>
  <a class="list-group-item <%if(location=="Virar"){%>active<%}%>" href="/branches/Virar"><li >Virar</li></a>
  <a class="list-group-item <%if(location=="Vaitarna"){%>active<%}%>" href="/branches/Vaitarna"><li >Vaitarna</li></a>
  <a class="list-group-item <%if(location=="Saphale"){%>active<%}%>" href="/branches/Saphale"><li ></li>Saphale</a>
  <a class="list-group-item <%if(location=="KelvaRoad"){%>active<%}%>" href="/branches/KelvaRoad"><li >Kelva Road</li></a>
  <a class="list-group-item <%if(location=="Palghar"){%>active<%}%>" href="/branches/Palghar"><li >Palghar</li></a>
    
            
</ul>
        
      </div>
 
          
          
      
          <div class="col col-xl-10 col-lg-10 col-md-9 col-sm-9 col-xs-12"
               style="background-color:white;height:100%;text-align:center;float:right;"> 
          
<%branches.forEach(function(branch){%>
            
 <div class="col col-xl-12 col-lg-12 col-md-4 col-sm-6 col-xs-12">
    
    <div style="border:solid 1px #ccc;margin-top:20px;">
      <i class="fa fa-university" style="font-size:70px;margin-top:50px"></i>
         <p><strong style="color:black;font-size:20px">
      <%=branch.BranchName%></strong></p><br>
    <span style="font-size:17px">
      <span style="color:blue">Location : </span><%=branch.Location%></span><br><br><br>
    
    <span style="font-size:15px">
      <span style="color:green">Vacancy : </span>Not Available</span>
    </div>
 
        </div> 
<%})%>
      </div>
    </div>
    
    
  </body>
  
</html>

 




